{
	"name": "fortibit",
	"version": "1.1.0",
	"description": "Password manager",
	"repository": {
		"type": "git",
		"url": "https://github.com/apolgesek/fortibit.git"
	},
	"author": {
		"name": "Arkadiusz Półgęsek",
		"email": "arkadiusz.polgesek@gmail.com"
	},
	"main": "main.js",
	"private": true,
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"ng": "ng",
		"start": "npm-run-all -p ng:serve electron:serve",
		"build": "npm run electron:serve-tsc && ng build --base-href ./",
		"build:dev": "npm run build -- -c dev",
		"build:prod": "npm run build -- -c production",
		"build:e2e": "npm run build -- -c e2e",
		"ng:serve": "ng serve -c local",
		"electron": "electron",
		"electron:serve-tsc": "tsc -p tsconfig.serve.json",
		"electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
		"electron:local": "npm run build:prod && electron .",
		"electron:windows": "node release-scripts/clean.js && npm run build:prod && electron-builder build --dir && node release-scripts/package.js && makensis release-scripts/nsis/Fortibit.nsi && makensis release-scripts/nsis/Fortibit-update.nsi",
		"electron:mac": "npm run build:prod && electron-builder build --mac",
		"test": "jest",
		"test-renderer": "ng test --watch=false",
		"test-renderer:watch": "ng test",
		"e2e": "npm run build:e2e && playwright test -c e2e/playwright.config.ts e2e/",
		"e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
		"version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
		"lint": "ng lint",
		"rebuild-native": "node release-scripts/rebuild-native.js"
	},
	"dependencies": {
		"angular-feather": "6.5.0",
		"bindings": "1.5.0",
		"csv-parser": "3.0.0",
		"csv-stringify": "6.4.4",
		"deepmerge": "4.3.1",
		"dexie": "3.2.4",
		"dexie-export-import": "1.0.3",
		"fast-xml-parser": "4.2.4",
		"focus-trap": "6.7.3",
		"fs-extra": "10.0.0",
		"generate-password": "1.7.0",
		"inversify": "6.0.2",
		"is-electron": "2.2.2",
		"module-alias": "2.2.3",
		"psl": "1.9.0",
		"reflect-metadata": "0.2.1",
		"rxjs": "7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "0.13.0",
		"zxcvbn": "4.4.2"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "16.0.1",
		"@angular-devkit/build-angular": "16.2.10",
		"@angular-eslint/builder": "16.3.1",
		"@angular-eslint/eslint-plugin": "16.3.1",
		"@angular-eslint/eslint-plugin-template": "16.3.1",
		"@angular-eslint/schematics": "16.3.1",
		"@angular-eslint/template-parser": "16.3.1",
		"@angular-eslint/utils": "16.3.1",
		"@angular/animations": "16.2.12",
		"@angular/cdk": "16.2.12",
		"@angular/cli": "16.2.10",
		"@angular/common": "16.2.12",
		"@angular/compiler": "16.2.12",
		"@angular/compiler-cli": "16.2.12",
		"@angular/core": "16.2.12",
		"@angular/forms": "16.2.12",
		"@angular/language-service": "16.2.12",
		"@angular/platform-browser": "16.2.12",
		"@angular/platform-browser-dynamic": "16.2.12",
		"@angular/router": "16.2.12",
		"@playwright/test": "1.40.1",
		"@types/css-font-loading-module": "0.0.7",
		"@types/jest": "29.5.7",
		"@types/node": "20.2.5",
		"@types/zxcvbn": "4.4.1",
		"conventional-changelog-cli": "2.1.1",
		"electron": "27.3.5",
		"electron-builder": "23.6.0",
		"electron-reloader": "1.2.3",
		"eslint": "8.42.0",
		"jest": "29.7.0",
		"node-polyfill-webpack-plugin": "1.1.4",
		"npm-run-all": "4.1.5",
		"playwright": "1.40.1",
		"prettier": "3.1.1",
		"ts-jest": "29.1.1",
		"ts-node": "10.9.1",
		"typescript": "5.1.6",
		"wait-on": "6.0.0",
		"webdriver-manager": "12.1.8"
	},
	"engines": {
		"node": ">=16.14.0"
	},
	"browserslist": [
		"chrome 100"
	]
}
