{
  "name": "fortibit",
  "version": "1.1.0",
  "description": "Password manager",
  "repository": {
    "type": "git",
    "url": "https://github.com/apolgesek/fortibit.git"
  },
  "author": {
    "name": "Arkadiusz Półgęsek",
    "email": "arkadiusz.polgesek@gmail.com"
  },
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "build:e2e": "npm run build -- -c e2e",
    "ng:serve": "ng serve -c local",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:windows": "node release-scripts/clean.js && npm run build:prod && electron-builder build --dir && node release-scripts/package.js && makensis release-scripts/nsis/Fortibit.nsi && makensis release-scripts/nsis/Fortibit-update.nsi",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:e2e && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "dependencies": {
    "croner": "5.3.5",
    "csv-parser": "3.0.0",
    "dexie": "3.2.2",
    "dexie-export-import": "1.0.3",
    "fast-xml-parser": "4.0.1",
    "focus-trap": "6.7.3",
    "fs-extra": "10.0.0",
    "generate-password": "^1.6.1",
    "primeicons": "5.0.0",
    "psl": "1.9.0",
    "rxjs": "7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "0.12.0",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "15.0.0-beta.0",
    "@angular-devkit/build-angular": "15.0.2",
    "@angular-eslint/builder": "15.0.0",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/schematics": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/animations": "15.0.0",
    "@angular/cdk": "15.0.0",
    "@angular/cli": "15.0.4",
    "@angular/common": "15.0.0",
    "@angular/compiler": "15.0.0",
    "@angular/compiler-cli": "15.0.0",
    "@angular/core": "15.0.0",
    "@angular/forms": "15.0.0",
    "@angular/language-service": "15.0.0",
    "@angular/platform-browser": "15.0.0",
    "@angular/platform-browser-dynamic": "15.0.0",
    "@angular/router": "15.0.0",
    "@playwright/test": "1.28.1",
    "@types/css-font-loading-module": "0.0.7",
    "@types/node": "18.11.0",
    "@types/zxcvbn": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "conventional-changelog-cli": "2.1.1",
    "electron": "22.0.2",
    "electron-builder": "23.6.0",
    "electron-reloader": "1.2.3",
    "eslint": "8.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsdoc": "37.0.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "node-polyfill-webpack-plugin": "1.1.4",
    "npm-run-all": "4.1.5",
    "playwright": "1.27.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.2",
    "wait-on": "6.0.0",
    "webdriver-manager": "12.1.8"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "browserslist": [
    "chrome 100"
  ]
}
